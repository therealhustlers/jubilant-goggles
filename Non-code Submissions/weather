#include <stdio.h>
void main()
{
    int outside,weather;
    printf("\nEnter if outside 1 true or 0 false");
    scanf("%d",&outside);
    printf("\nEnter if rain 1 true or 0 false");
    scanf("%d",&weather);
    if (outside && weather)
    printf("\nPlease use an umbrella\n");
    else
    printf("\n Dress normally\n\n");
}
